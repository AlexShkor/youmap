@model IEnumerable<EventListItem>

@{
    Layout = "~/Areas/Mobile/Views/Shared/_Layout.Mobile.cshtml";
    ViewBag.Title = "Встречи";
}
<ul data-role="listview" data-inset="true" data-filter="true">
@foreach(var item in Model)
{

        <li data-corners="false" data-shadow="false" 
        data-icon="arrow-r" data-iconpos="right" class="ui-btn ui-btn-icon-right ui-li-has-arrow ui-li ui-btn-up">
        <div>
            <div class="ui-btn-text">
                <a href="@item.Url" class="ui-link-inherit">
                    <p class="ui-li-aside ui-li-desc"><strong>@item.StartDate</strong></p>
                    <h3 class="ui-li-heading">@item.Title</h3>
                    <p class="ui-li-desc">@item.PlaceTitle</p>
                </a>
            </div>
            <span class="ui-icon ui-icon-arrow-r ui-icon-shadow">&nbsp;</span>
        </div>
    </li>
}
</ul>