@model FeedDetailsModel

<div class="well">
    <h2>#@Model.Name</h2>
    @if(!Model.IsSubscribed)
    {
        <a  class="btn btn-primary ajax-link"  href="@Url.Action("Subscribe",new{name=Model.Name})">Подписаться</a>
    }
    else
    {
        <a class="btn btn-danger ajax-link" href="@Url.Action("Unsubscribe",new{name=Model.Name})">Отменить подписку</a>
    }
</div>