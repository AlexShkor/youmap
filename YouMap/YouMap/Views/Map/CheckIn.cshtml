@model YouMap.Models.CheckInModel

<script>
    $(document).ready(function () {
        YouMap.Vk.Panel.CheckInInit();
    });
</script> 

<div id="checkin" class="checkin">
    @using(Html.BeginForm("CheckIn","Map"))
    {
            <div>
                @Html.HiddenFor(x => x.PlaceId)
                <label for="Memo">@Model.Title</label>
            </div>
        <div>
            @Html.HiddenFor(x=> x.Latitude)
            @Html.HiddenFor(x=> x.Longitude)
            @Html.TextAreaFor(x => x.Memo)
            <p>
                <input type="submit" class="ajax-submit awesome small" value="Отметится" />
            </p>
        </div>
    }
</div>