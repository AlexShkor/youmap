@using System.Web.Mvc
@model YouMap.Models.PlaceSearchModel
<script>
    $(document).ready(function () {
        YouMap.Search.Initialize('@Url.Action("Search","Places")');

    })
</script>
@using (Html.BeginForm("Search", "Places", FormMethod.Post, new { id = "searchForm" }))
{
    @Html.TextBoxFor(x => x.Serach, new { id = "searchField" })
    @Html.HiddenFor(x => x.UserLocation.Latitude)
    @Html.HiddenFor(x => x.UserLocation.Longitude)
    <input id="searchSubmit" class="search-google-btn" name="submit" type="button" value="Искать" />
}