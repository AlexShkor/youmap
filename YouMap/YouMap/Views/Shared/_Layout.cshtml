<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    
    @Html.Partial("Styles")
    @Html.Partial("Scripts")
</head>
<body>
    <div class="page">
        <header>
            @Html.Action("Navigation", "Categories")
            <div id="title">
                <a href="/"><h1>YOUMAP</h1></a>
                @*@Html.Partial("AjaxLoader")*@
            </div>
            <div id="topBar">
                @Html.Partial("TopBar")
            </div>
        </header>
        <section id="main">
            @RenderBody()
        </section>
    </div>
</body>
</html>
