
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @Html.Partial("Scripts")
</head>
<body>
    <div class="page">
        <header>
            @Html.Action("Navigation", "Categories")
            <div id="title">
                <h1>YOUMAP</h1>
            </div>
            <div class="top-bar">
                @Html.Partial("TopBar")
            </div>
            
        </header>
        <section id="main">
            @RenderBody()
        </section>
    </div>
</body>
</html>
